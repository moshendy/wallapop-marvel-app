pod 'ObjectMapper'

  target 'Wallapop Marvel AppTests' do
    pod 'Alamofire'
    pod 'SwiftyJSON'
    pod 'ObjectMapper'
  end
  
  target 'Wallapop Marvel AppSnapShotsTests' do
    pod 'SnapshotTesting', '~> 1.8.1'
  end

  
  
end
